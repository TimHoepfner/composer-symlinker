{
    "name": "timhoepfner/composer-symlinker",
    "type": "composer-plugin",
    "description": "A composer plugin to install packages as local symbolic links",
    "require": {
        "composer-plugin-api": "1.*",
        "vlucas/phpdotenv": "1.*"
    },
    "require-dev": {
        "composer/composer": "1.*",
        "phpunit/phpunit": "~4.0"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": {
          "ComposerSymlinker": "src/"
        }
    },
    "extra": {
        "class": "ComposerSymlinker\\SymlinkerPlugin",
        "branch-alias": {
            "dev-master": "1.0.*-dev"
        }
    }
}
